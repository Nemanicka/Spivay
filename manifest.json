{
  "name": "Constant Context",
  "description" : "Highlights elements with keywords on developer.chrome",
  "version": "1.0",
  "page_action": {
  "default_icon": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png"
  },
  "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/get_started16.png",
    "48": "images/get_started48.png",
    "32": "images/get_started32.png",
    "128": "images/get_started128.png"
  },
  "permissions": [
    "https://twitter.com/*",
    "https://publish.twitter.com/*",
    "https://platform.twitter.com/*",
    "https://github.com/*",
    "tabs",
    "activeTab",
    "storage",
    "declarativeContent"
  ],
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://surikov.github.io/ ; object-src 'self'",
  "background": {
    "scripts": ["background.js", "Tone.js", "StartAudioContext.js", "Rhythm.js", "instruments.js", "widgets.js"],
    "persistent": true
  },
  "web_accessible_resources": ["style.css"],
  "content_scripts": [
  {
  "js": ["jquery.min.js", "content_script.js", "jquery.js"],
  "matches": ["https://twitter.com/*", "http://www.twitter.com/*"],
  "run_at": "document_end"
  }
  ]
}
